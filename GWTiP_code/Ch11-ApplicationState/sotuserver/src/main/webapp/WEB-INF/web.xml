<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>State of the Union</display-name>
    
    <description>A Chat and Screen Sharing Application</description>
    <servlet>
        <servlet-name>ImagePublishingServlet</servlet-name>
        <servlet-class>gwtip.sotu.server.ImagePublishingServlet</servlet-class>
    </servlet>
    
    
    <servlet>
        <servlet-name>Images</servlet-name>
        <servlet-class>gwtip.sotu.server.Images</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ImagePublishingServlet</servlet-name>
        <url-pattern>/ImagePublishingServlet</url-pattern>
    </servlet-mapping>
    
    
    <!--servlet-mapping>
        <servlet-name>Images</servlet-name>
        <url-pattern>/images*</url-pattern>
    </servlet-mapping-->
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
    </web-app>
